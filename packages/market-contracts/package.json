{
  "name": "@meemproject/market-contracts",
  "version": "0.3.0",
  "private": false,
  "main": "./index.ts",
  "files": [
    "abi",
    "addresses",
    "typechain",
    "types",
    "package.json",
    "README.md",
    "CHANGELOG.md",
    "index.ts"
  ],
  "scripts": {
    "build": "yarn build:sol && yarn createABI && yarn generateTypes",
    "clean": "rm -rf cache artifacts abi dist typechain types",
    "clean:modules": "rm -rf node_modules",
    "build:sol": "hardhat compile",
    "test": "hardhat test --network hardhat",
    "verify": "hardhat verify",
    "lint": "eslint --max-warnings=0 --ext=.ts,.tsx . && tsc -p . --noEmit",
    "lint:fix": "eslint --fix --ext=.ts,.tsx .",
    "release": "semantic-release",
    "size": "hardhat size-contracts",
    "deploy": "hardhat deploy",
    "upgradeFacet": "hardhat upgradeFacet",
    "network": "hardhat node",
    "console": "hardhat console",
    "watch": "concurrently -r --kill-others \"chokidar 'contracts/**/*.sol' -c 'yarn build:sol'\"",
    "createABI": "tsnd --transpile-only scripts/createABI.ts",
    "generateTypes": "typechain --target ethers-v5  './types/MeemMarket.json'",
    "link": "yarn link"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@float-capital/solidity-coverage": "^0.7.17",
    "@kengoldfarb/semantic-release-config": "^1.0.3",
    "@nomiclabs/hardhat-ethers": "^2.0.3",
    "@nomiclabs/hardhat-etherscan": "^2.1.8",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts-upgradeable": "^4.4.0",
    "@openzeppelin/hardhat-upgrades": "^1.12.0",
    "@typechain/ethers-v5": "^10.0.0",
    "@typechain/hardhat": "^6.0.0",
    "@types/chai": "^4.2.15",
    "@types/chai-as-promised": "^7.1.3",
    "@types/fs-extra": "^9.0.13",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.35",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^6.4.0",
    "eslint": "^8.13.0",
    "eslint-config-kengoldfarb": "1.4.1",
    "ethereum-waffle": "^3.3.0",
    "ethers": "^5.6.4",
    "hardhat": "^2.7.0",
    "hardhat-abi-exporter": "^2.3.1",
    "hardhat-contract-sizer": "^2.1.1",
    "hardhat-gas-reporter": "^1.0.4",
    "hardhat-typechain": "^0.3.5",
    "prettier": "^2.5.0",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typechain": "^8.0.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.4.0",
    "@solidstate/contracts": "^0.0.28",
    "dotenv": "^10.0.0",
    "fs-extra": "^10.0.0",
    "minimist": "^1.2.5",
    "solidity-coverage": "^0.7.16"
  }
}
