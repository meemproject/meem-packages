{
	"name": "@meemproject/utils",
	"version": "0.10.0",
	"main": "build/index.js",
	"license": "MIT",
	"files": [
		"build",
		"package.json",
		"README.md",
		"CHANGELOG.md",
		"yarn.lock"
	],
	"scripts": {
		"clean": "rm -rf build/*",
		"clean:modules": "rm -rf node_modules",
		"build": "npm run clean ; tsc ; echo \"Finished Building\"",
		"lint": "eslint --max-warnings=0 --ext=.ts,.tsx . && tsc -p . --noEmit",
		"lint:fix": "eslint --fix --ext=.ts,.tsx .",
		"watch": "yarn run build && chokidar 'src/**/*.(js|jsx|ts|tsx)' -c 'yarn run build'",
		"link": "yarn link"
	},
	"devDependencies": {
		"@types/fs-extra": "^9.0.13",
		"@types/node": "^17.0.23",
		"@types/superagent": "^4.1.15",
		"chokidar-cli": "^3.0.0",
		"eslint": "^8.13.0",
		"eslint-config-kengoldfarb": "^1.4.5",
		"fs-extra": "^10.0.1",
		"superagent": "^7.1.2",
		"ts-node": "^10.7.0",
		"ts-node-dev": "^1.1.8",
		"typescript": "^4.6.3"
	},
	"dependencies": {
		"@kengoldfarb/log": "^1.0.5",
		"@meemproject/meem-contracts": "^0.10.0",
		"ethers": "^5.6.4"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	}
}
