{
  "name": "@meemproject/react",
  "version": "0.17.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/react.esm.js",
  "files": [
    "dist",
    "generated"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "clean": "rm -rf build/*",
    "clean:modules": "rm -rf node_modules",
    "watch:code": "tsdx watch --transpileOnly",
    "watch:gql": "chokidar 'src/gql/**/*.(js|jsx|ts|tsx)' -c 'yarn run codegen'",
    "watch": "concurrently \"yarn run watch:code\" \"yarn run watch:gql\"",
    "build": "tsdx build --transpileOnly",
    "test": "tsdx test --passWithNoTests",
    "#lint": "tsdx lint",
    "lint": "eslint --max-warnings=0 --ext=.ts,.tsx ./src && tsc -p . --noEmit",
    "lint:fix": "eslint --fix --ext=.ts,.tsx .",
    "#prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "link": "yarn link",
    "codegen": "graphql-codegen -r dotenv/config --config codegen.js"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@graphql-codegen/cli": "^2.13.12",
    "@graphql-codegen/introspection": "^2.2.1",
    "@graphql-codegen/typescript": "^2.8.1",
    "@graphql-codegen/typescript-operations": "^2.5.6",
    "@graphql-codegen/typescript-react-apollo": "^3.3.6",
    "@rushstack/eslint-patch": "^1.1.3",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/addons": "^6.4.22",
    "@storybook/react": "^6.4.22",
    "@types/js-cookie": "^3.0.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.181",
    "@types/luxon": "^3.0.0",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.1",
    "@types/superagent": "^4.1.15",
    "babel-loader": "^8.2.4",
    "dotenv": "^16.0.3",
    "eslint": "^8.13.0",
    "eslint-config-kengoldfarb": "^1.4.5",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-is": "^18.0.0",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@apollo/client": "^3.7.1",
    "@auth0/auth0-react": "^1.12.0",
    "@emotion/react": "^11.10.0",
    "@emotion/server": "^11.10.0",
    "@kengoldfarb/log": "^1.0.5",
    "@magic-ext/connect": "^6.1.0",
    "@mantine/core": "^5.7.2",
    "@mantine/hooks": "^5.7.2",
    "@material-ui/core": "^4.12.4",
    "@material-ui/lab": "^4.0.0-alpha.61",
    "@meemproject/meem-contracts": "^0.17.0",
    "@meemproject/sdk": "^0.17.0",
    "@meemproject/utils": "^0.17.0",
    "@mui/material": "^5.6.1",
    "@walletconnect/web3-provider": "^1.7.7",
    "ethers": "^5.6.4",
    "graphql": "^16.6.0",
    "graphql-ws": "^5.11.2",
    "js-cookie": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "luxon": "^3.0.1",
    "magic-sdk": "^13.1.0",
    "superagent": "^7.1.2",
    "swr": "^1.3.0",
    "web3modal": "^1.9.9"
  },
  "size-limit": [
    {
      "path": "dist/react.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react.esm.js",
      "limit": "10 KB"
    }
  ]
}
