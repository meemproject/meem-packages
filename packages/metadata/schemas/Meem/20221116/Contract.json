{
    "description": "This schema describes the 20221116 version of the Meem Contract metadata standard.",
    "title": "Meem_Contract_20221116",
    "$id": "Meem/20221116/Contract.json",
    "type": "object",
    "properties": {
        "meem_metadata_type": {
            "description": "The type of metadata. See supported Meem metadata types here: https://meem.wtf",
            "examples": [
                "Meem_Contract"
            ],
            "type": "string"
        },
        "meem_metadata_version": {
            "description": "The calendar version of the Meem metadata schema. See supported Meem metadata schemas here: https://meem.wtf",
            "examples": [
                "20221116"
            ],
            "type": "string"
        },
        "description": {
            "description": "The description of the contract.",
            "examples": [
                "Meemsters Union - Meemsters Unite!"
            ],
            "type": "string"
        },
        "name": {
            "description": "The name of the contract.",
            "examples": [
                "Meemsters Union"
            ],
            "type": "string"
        },
        "image": {
            "description": "An optional image for the contract.",
            "examples": [
                "https://ipfs.io/ipfs/1234"
            ],
            "type": "string"
        },
        "external_url": {
            "type": "string",
            "description": "An optional url to a website with more information about the contract.",
            "examples": [
                "https://clubs.link/meemsters-union"
            ]
        }
    },
    "examples": [
        {
            "meem_metadata_type": "meem-contract",
            "meem_metadata_version": "20221116",
            "name": "Meemsters Union",
            "description": "Meemsters Unite!",
            "image": "https://ipfs.io/ipfs/1234",
            "external_url": "https://clubs.link/meemsters-union"
        }
    ],
    "required": [
        "meem_metadata_type",
        "meem_metadata_version",
        "name",
        "description"
    ]
}