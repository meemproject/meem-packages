{
    "description": "This schema describes version 20221116 of the Meem Token metadata standard.",
    "title": "Meem_Token_20221116",
    "$id": "Meem/20221116/Token.json",
    "type": "object",
    "properties": {
        "meem_metadata_type": {
            "description": "The type of metadata. See supported Meem metadata types here: https://meem.wtf",
            "examples": [
                "Meem_Token"
            ],
            "type": "string"
        },
        "meem_metadata_version": {
            "description": "The calendar version of the Meem token metadata schema. See supported Meem contract metadata schemas here: https://meem.wtf",
            "examples": [
                "20221116"
            ],
            "type": "string"
        },
        "description": {
            "description": "The description of the token.",
            "examples": [
                "This token verifies the owner is a Meemsters Union member."
            ],
            "type": "string"
        },
        "name": {
            "description": "The name of the token.",
            "examples": [
                "Meemsters Union Club Token"
            ],
            "type": "string"
        },
        "image": {
            "description": "An optional image for the token.",
            "examples": [
                "https://ipfs.io/ipfs/1234"
            ],
            "type": "string"
        },
        "external_url": {
            "type": "string",
            "description": "An optional url containing additional information about the token.",
            "examples": [
                "https://clubs.link/meemsters-union/1"
            ]
        }
    },
    "examples": [
        {
            "meem_metadata_type": "Meem_Token",
            "meem_metadata_version": "20221116",
            "name": "Meemsters Union",
            "description": "Meemsters Unite!",
            "image": "https://ipfs.io/ipfs/1234",
            "external_url": "https://clubs.link/meemsters-union"
        }
    ],
    "required": [
        "meem_metadata_type",
        "meem_metadata_version",
        "name",
        "description"
    ]
}