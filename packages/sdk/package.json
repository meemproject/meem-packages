{
	"name": "@meemproject/sdk",
	"version": "0.14.0",
	"main": "build/index.js",
	"license": "MIT",
	"files": [
		"build",
		"package.json",
		"README.md",
		"CHANGELOG.md",
		"yarn.lock"
	],
	"scripts": {
		"clean": "rm -rf build/*",
		"clean:modules": "rm -rf node_modules",
		"build": "npm run clean ; tsc ; echo \"Finished Building\"",
		"fetchTypes": "tsnd --project tsconfig.json --transpile-only src/scripts/fetchTypes.ts",
		"fetchAgreementTypes": "tsnd --project tsconfig.json --transpile-only src/scripts/fetchAgreementTypes.ts",
		"generateTypes": "typechain --target=ethers-v5 'src/abis/*.json' --out-dir='src/abis'",
		"fetchMeemABI": "tsnd --project tsconfig.scripts.json --transpile-only src/scripts/fetchMeemABI.ts && yarn generateTypes",
		"lint": "eslint --max-warnings=0 --ext=.ts,.tsx . && tsc -p . --noEmit",
		"lint:fix": "eslint --fix --ext=.ts,.tsx .",
		"watch": "yarn run build && chokidar 'src/**/*.(js|jsx|ts|tsx)' -c 'yarn run build'",
		"link": "yarn link",
		"codegen": "graphql-codegen -r dotenv/config --config codegen.js"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "^2.14.1",
		"@graphql-codegen/introspection": "^2.2.1",
		"@graphql-codegen/typescript-operations": "^2.5.7",
		"@nomiclabs/hardhat-ethers": "^2.1.1",
		"@nomiclabs/hardhat-etherscan": "^3.1.0",
		"@nomiclabs/hardhat-waffle": "^2.0.3",
		"@typechain/ethers-v5": "^10.0.0",
		"@typechain/hardhat": "^6.0.0",
		"@types/chai": "^4.2.22",
		"@types/chai-as-promised": "^7.1.4",
		"@types/fs-extra": "^9.0.13",
		"@types/mocha": "^9.0.0",
		"@types/node": "^17.0.23",
		"@types/slug": "^5.0.3",
		"@types/superagent": "^4.1.15",
		"@types/uuid": "^9.0.0",
		"chai": "^4.3.4",
		"chai-as-promised": "^7.1.1",
		"chokidar-cli": "^3.0.0",
		"eslint": "^8.13.0",
		"eslint-config-kengoldfarb": "^1.4.5",
		"fs-extra": "^10.0.1",
		"hardhat": "^2.10.2",
		"hardhat-abi-exporter": "^2.10.0",
		"hardhat-contract-sizer": "^2.6.1",
		"hardhat-gas-reporter": "^1.0.4",
		"hardhat-typechain": "^0.3.5",
		"mocha": "^10.1.0",
		"ts-node": "^10.7.0",
		"ts-node-dev": "^1.1.8",
		"typescript": "^4.6.3"
	},
	"dependencies": {
		"@apollo/client": "^3.7.1",
		"@kengoldfarb/log": "^1.0.5",
		"@lit-protocol/constants": "^2.0.35",
		"@lit-protocol/lit-node-client": "^2.0.35",
		"@meemproject/meem-contracts": "^0.14.0",
		"@meemproject/utils": "^0.14.0",
		"@tableland/sdk": "^3.1.5",
