{
	"name": "@meemproject/api",
	"version": "0.0.11",
	"main": "index.ts",
	"scripts": {
		"clean": "rm -rf build/*",
		"build": "npm run clean ; tsc ; echo \"Finished Building\"",
		"fetchTypes": "tsnd --project tsconfig.json --transpile-only scripts/fetchTypes.ts",
		"generateTypes": "typechain --target=ethers-v5 'src/abis/*.json' --out-dir='src/abis'",
		"fetchMeemABI": "tsnd --project tsconfig.scripts.json --transpile-only scripts/fetchMeemABI.ts && yarn generateTypes",
		"fetchMeemIdABI": "tsnd --project tsconfig.scripts.json --transpile-only scripts/fetchMeemIdABI.ts && yarn generateTypes",
		"lint": "eslint --max-warnings=0 --ext=.ts,.tsx . && tsc -p . --noEmit",
		"lint:fix": "eslint --fix --ext=.ts,.tsx ."
	},
	"devDependencies": {
		"@types/fs-extra": "^9.0.13",
		"@types/node": "^17.0.23",
		"@types/superagent": "^4.1.15",
		"eslint": "^7.32.0",
		"eslint-config-kengoldfarb": "^1.3.0",
		"fs-extra": "^10.0.1",
		"superagent": "^7.1.2",
		"ts-node": "^10.7.0",
		"ts-node-dev": "^1.1.8",
		"typescript": "^4.6.3"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"dependencies": {
		"@typechain/ethers-v5": "^10.0.0",
		"ethers": "^5.6.2",
		"tsnd": "^1.0.2",
		"typechain": "^8.0.0"
	}
}
