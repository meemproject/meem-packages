{
	"name": "@meemproject/meem-contracts",
	"version": "0.7.9",
	"description": "Meems are pieces of digital content wrapped in more advanced dynamic property rights. They are ideas, stories, images -- existing independently from any social platform -- whose creators have set the terms by which others can access, remix, and share in their value.",
	"author": "Meem Project",
	"homepage": "https://meem.wtf",
	"license": "MIT",
	"main": "./dist/index.js",
	"files": [
		"typechain",
		"types",
		"package.json",
		"README.md",
		"CHANGELOG.md",
		"dist",
		"versions"
	],
	"scripts": {
		"build": "yarn build:sol && yarn build:ts",
		"test:build": "waffle",
		"clean": "concurrently \"rm -rf cache artifacts dist typechain types\"",
		"clean:modules": "rm -rf node_modules",
		"#build:sol": "TS_NODE_TRANSPILE_ONLY=true hardhat compile && yarn createABI && yarn generateTypes && echo 'build:sol Finished'",
		"build:sol": "TS_NODE_TRANSPILE_ONLY=true hardhat compile && echo 'build:sol Finished'",
		"build:ts": "tsc -p ./tsconfig.build.json && echo 'build:ts Finished'",
		"hardhat": "hardhat",
		"accounts": "hardhat accounts",
		"deployDiamond": "TS_NODE_TRANSPILE_ONLY=true DEBUG=@meemproject/* hardhat deployDiamond",
		"upgradeDiamond": "TS_NODE_TRANSPILE_ONLY=true DEBUG=@meemproject/* hardhat upgradeDiamond",
		"upgradeFacet": "TS_NODE_TRANSPILE_ONLY=true DEBUG=@meemproject/* hardhat upgradeFacet",
		"test": "TS_NODE_TRANSPILE_ONLY=true hardhat test --network hardhat",
		"coverage": "hardhat coverage",
		"network": "TS_NODE_TRANSPILE_ONLY=true hardhat node",
		"console": "TS_NODE_TRANSPILE_ONLY=true hardhat console",
		"epmUpload": "TS_NODE_TRANSPILE_ONLY=true hardhat epmUpload",
		"watch": "concurrently -r --kill-others \"chokidar 'contracts/**/*.sol' -c 'yarn build:sol'\" \"chokidar 'src/**/*.ts' -c 'yarn build:ts'\"",
		"verify": "hardhat verify",
		"createVersion": "DEBUG=@meemproject/* hardhat createVersion",
		"lint": "concurrently \"yarn lint:ts\" \"yarn lint:sol\" \"yarn lint:tsc\"",
		"lint:ts": "eslint --max-warnings=0 --ext=.ts,.tsx .",
		"lint:tsc": "tsc -p ./tsconfig.json --noEmit",
		"lint:sol": "solhint ./contracts/**/*.sol",
		"lint:fix": "eslint --fix --ext=.ts,.tsx .",
		"size": "TS_NODE_TRANSPILE_ONLY=true hardhat size-contracts",
		"createABI": "tsnd --transpile-only scripts/createABI.ts",
		"generateTypes": "typechain --target ethers-v5  './types/Meem.json' --out-dir ./types",
		"solregex": "solregex",
		"link": "yarn link"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"devDependencies": {
		"@float-capital/solidity-coverage": "^0.7.17",
		"@nomiclabs/hardhat-ethers": "^2.0.2",
		"@nomiclabs/hardhat-etherscan": "^2.1.2",
		"@nomiclabs/hardhat-waffle": "^2.0.1",
		"@solidstate/contracts": "^0.0.35",
		"@typechain/ethers-v5": "^10.0.0",
		"@typechain/hardhat": "^6.0.0",
		"@types/chai": "^4.2.22",
		"@types/chai-as-promised": "^7.1.4",
		"@types/express": "^4.17.11",
		"@types/fs-extra": "^9.0.13",
		"@types/lodash": "^4.14.182",
		"@types/luxon": "^2.3.1",
		"@types/mocha": "^9.0.0",
		"@types/node": "^16.10.2",
		"@types/prompt": "^1.1.0",
		"@types/superagent": "^4.1.15",
		"base64-sol": "^1.0.1",
		"chai": "^4.3.4",
		"chai-as-promised": "^7.1.1",
		"chokidar-cli": "^3.0.0",
		"concurrently": "^6.2.1",
		"dotenv": "^10.0.0",
		"eslint": "^8.13.0",
		"eslint-config-kengoldfarb": "^1.4.2",
		"ethereum-waffle": "^3.3.0",
		"fs-extra": "^10.0.0",
		"hardhat": "^2.7.0",
		"hardhat-abi-exporter": "^2.3.1",
		"hardhat-contract-sizer": "^2.1.1",
		"hardhat-gas-reporter": "^1.0.4",
		"hardhat-typechain": "^0.3.5",
		"lodash": "^4.17.21",
		"luxon": "^2.3.2",
		"prettier": "^2.4.1",
		"prettier-plugin-solidity": "^1.0.0-beta.12",
		"prompt": "^1.1.0",
		"solhint": "^3.3.7",
		"solregex": "^0.3.1",
		"ts-node": "^10.0.0",
		"ts-node-dev": "^1.1.8",
		"typechain": "^8.0.0",
		"typescript": "^4.6.3"
	},
	"dependencies": {
		"@kengoldfarb/log": "^1.0.5",
		"ethers": "^5.6.4",
		"superagent": "^8.0.0"
	}
}
